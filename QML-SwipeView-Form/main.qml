import QtQuick 2.15
import QtQuick.Window 2.15
import QtQuick.Controls 2.5
import QtQuick.Layouts 1.2

Window {
    width: 320
    height: 480
    visible: true
    title: qsTr("Swipe In Small Window")

    Rectangle {
        width: 300
        height: 200
        color: "transparent"
        border.color: "black"
        border.width: 2
        radius: 10
        anchors.centerIn: parent

        SwipeView {
            id: view
            interactive: SwipeView.ForceElasticity
            currentIndex: 1
            anchors.fill: parent

            Item {
                Rectangle {
                      width: parent.width
                      height: parent.height
                      Image {
                          source: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTQr3XftMvBt0wCVrv0UoMVsBDE7MRV3-s0XHErYy3fzH9kwwTVoQ8z8TLqp8Mo1Zj_BZI&usqp=CAU"
                          anchors.fill: parent
                          fillMode: Image.PreserveAspectFit
                }
            }
            }
            Item {
                Rectangle {
                      width: parent.width
                      height: parent.height
                      Image {
                          source: "data:image/jpeg;base64,"
                          anchors.fill: parent
                          fillMode: Image.PreserveAspectFit
                }
            }
            }
            Item {
                Rectangle {
                      width: parent.width
                      height: parent.height
                      Image {
                          source: "data:image/jpeg;base64,"
                          anchors.fill: parent
                          fillMode: Image.PreserveAspectFit
                }
            }
            }
        }
    }

    PageIndicator {
        id: indicator

        count: view.count
        currentIndex: view.currentIndex

        anchors.bottom: view.bottom
        anchors.horizontalCenter: parent.horizontalCenter
    }
}
